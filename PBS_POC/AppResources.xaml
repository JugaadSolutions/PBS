<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <Style x:Key="AppStyle" TargetType="Control">
        <Setter Property="Background" Value="Green"/>
        <Setter Property="FontFamily" Value="Arial" />
        <Setter Property="FontSize" Value="15" />
    </Style>

    <s:String x:Key="BannerText" xml:space="preserve">PBS Mysuru  &#x0a; POC App</s:String>

    <BitmapImage x:Key="Logo" CreateOptions="IgnoreImageCache"  CacheOption="OnLoad" UriSource="pack://application:,,,/Images/GWRLogo.bmp"  />
    <XmlDataProvider x:Key="MenuDataProvider" XPath="//Menus/Menu[@id='Menu1']" Source="Menus.xml"/>
    <CollectionViewSource x:Key="ReportGridData" />
</ResourceDictionary>